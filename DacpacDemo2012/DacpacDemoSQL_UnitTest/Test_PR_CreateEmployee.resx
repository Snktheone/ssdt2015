  <data name="dbo_PR_CreateEmployeeTest_TestAction.SqlScript" xml:space="preserve">
    <value>-- database unit test for dbo.PR_CreateEmployee
DECLARE @RC AS INT, @EmployeeId AS INT, @Employee_FirstName AS VARCHAR (255), @Employee_LastName AS VARCHAR (255), @Employee_Gender AS CHAR (1), @Employee_DateofBirth AS DATETIME, @Employee_HireDate AS DATETIME, @Employee_Designation AS VARCHAR (255), @Employee_DepartmentID AS INT, @Employee_ManagerID AS INT;

SELECT @RC = 0,
       @EmployeeId = 0,
       @Employee_FirstName = NULL,
       @Employee_LastName = NULL,
       @Employee_Gender = NULL,
       @Employee_DateofBirth = getdate(),
       @Employee_HireDate = getdate(),
       @Employee_Designation = NULL,
       @Employee_DepartmentID = 0,
       @Employee_ManagerID = 0;

EXECUTE @RC = [dbo].[PR_CreateEmployee] @EmployeeId, @Employee_FirstName, @Employee_LastName, @Employee_Gender, @Employee_DateofBirth, @Employee_HireDate, @Employee_Designation, @Employee_DepartmentID, @Employee_ManagerID;

SELECT @RC AS RC;

</value>
