  <data name="testInitializeAction.SqlScript" xml:space="preserve">
    <value>/*
Prepare Data for Test
*/

DECLARE @EmployeeId AS INT, @Employee_FirstName AS VARCHAR (255), @Employee_LastName AS VARCHAR (255), @Employee_Gender AS CHAR (1),
        @Employee_DateofBirth AS DATETIME, @Employee_HireDate AS DATETIME, @Employee_Designation AS VARCHAR (255), 
		@Employee_DepartmentID AS INT, @Employee_ManagerID AS INT;

SET @EmployeeId = 10000;
SET @Employee_FirstName = 'John';
SET @Employee_LastName = 'Anthony';
SET @Employee_Gender = 'M';
SET @Employee_DateofBirth = '19800823';
SET @Employee_HireDate = '20150101';
SET @Employee_Designation = 'Developer';
SET @Employee_DepartmentID = 1111;
SET @Employee_ManagerID = 70001;
 
SELECT 
        @EmployeeId AS EmployeeId, 
		@Employee_FirstName AS Employee_FirstName, 
		@Employee_LastName AS Employee_LastName, 
		@Employee_Gender AS Employee_Gender, 
		@Employee_DateofBirth AS Employee_DateofBirth, 
		@Employee_HireDate AS Employee_HireDate, 
		@Employee_Designation AS Employee_Designation, 
		@Employee_DepartmentID AS Employee_DepartmentID, 
		@Employee_ManagerID AS Employee_ManagerID



DELETE FROM dbo.tbl_Employee WHERE EmployeeId = @EmployeeId</value>
